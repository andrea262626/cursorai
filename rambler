            <div class="section">
                <h2 class="section-title">Trending This Week</h2>
                <div class="ai-prompts" style="justify-content: flex-start; flex-wrap: wrap; gap: 12px;">
                    <div class="prompt-chip">🎭 Theater weekends</div>
                    <div class="prompt-chip">🍷 Wine country escapes</div>
                    <div class="prompt-chip">🏔️ Mountain retreats</div>
                    <div class="prompt-chip">🌊 Coastal adventures</div>
                    <div class="prompt-chip">🎨 Art & culture dives</div>
                    <div class="prompt-chip">🍕 Foodie pilgrimages</div>
                </div>
            </div>
        </div>

        <div class="bottom-nav">
            <div class="nav-dot active"></div>
            <div class="nav-dot"></div>
            <div class="nav-dot"></div>
        </div>
    </div>

    <script>
        // Enhanced interactive behavior
        const searchInput = document.querySelector('.search-input');
        const promptChips = document.querySelectorAll('.prompt-chip');
        const ctaButtons = document.querySelectorAll('.cta-button');
        const happeningCards = document.querySelectorAll('.happening-card');
        const inspirationCards = document.querySelectorAll('.inspiration-card');

        // Enhanced placeholder rotation with smooth transitions
        const placeholders = [
            "Where can I go this weekend for...",
            "jazz, wine, and photography?",
            "live music and great food?",
            "creative workshops and art?",
            "mountain hiking and stargazing?",
            "craft beer and local culture?"
        ];

        let currentPlaceholder = 0;

        function fadeplaceholder() {
            searchInput.style.transition = 'opacity 0.5s ease-in-out';
            searchInput.style.opacity = '0.4';
            
            setTimeout(() => {
                searchInput.placeholder = placeholders[currentPlaceholder];
                currentPlaceholder = (currentPlaceholder + 1) % placeholders.length;
                searchInput.style.opacity = '1';
            }, 500);
        }

        // Start enhanced placeholder animation
        setTimeout(() => {
            fadeplaceholder();
            setInterval(fadeplaceholder, 3500);
        }, 1500);

        // Enhanced prompt chip interactions
        promptChips.forEach(chip => {
            chip.addEventListener('click', () => {
                // Add ripple effect
                const ripple = document.createElement('div');
                ripple.style.cssText = `
                    position: absolute;
                    border-radius: 50%;
                    background: rgba(30, 58, 138, 0.3);
                    width: 100px;
                    height: 100px;
                    left: 50%;
                    top: 50%;
                    transform: translate(-50%, -50%) scale(0);
                    animation: ripple 0.6s ease-out;
                    pointer-events: none;
                `;
                
                chip.style.position = 'relative';
                chip.style.overflow = 'hidden';
                chip.appendChild(ripple);
                
                setTimeout(() => ripple.remove(), 600);
                
                // Update search input
                searchInput.value = chip.textContent;
                searchInput.focus();
                
                // Add selected state
                promptChips.forEach(c => c.classList.remove('selected'));
                chip.classList.add('selected');
            });
        });

        // Enhanced button interactions with feedback
        ctaButtons.forEach(button => {
            button.addEventListener('click', () => {
                // Add click feedback
                button.style.transform = 'translateY(1px) scale(0.98)';
                setTimeout(() => {
                    button.style.transform = 'translateY(-2px) scale(1)';
                }, 150);
                
                // Simulate loading state
                const originalText = button.textContent;
                button.textContent = 'Loading...';
                button.style.background = 'linear-gradient(135deg, #64748b 0%, #94a3b8 100%)';
                
                setTimeout(() => {
                    button.textContent = originalText;
                    button.style.background = 'linear-gradient(135deg, #0D0D0D 0%, #374151 100%)';
                }, 2000);
            });
        });

        // Enhanced scroll interactions
        const happeningsScroll = document.querySelector('.happenings-scroll');
        let isScrolling = false;

        happeningsScroll.addEventListener('wheel', (e) => {
            if (!isScrolling) {
                isScrolling = true;
                e.preventDefault();
                
                const scrollAmount = e.deltaY * 1.2;
                happeningsScroll.scrollBy({
                    left: scrollAmount,
                    behavior: 'smooth'
                });
                
                setTimeout(() => {
                    isScrolling = false;
                }, 150);
            }
        });

        // Enhanced card animations on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const cardObserver = new IntersectionObserver((entries) => {
            entries.forEach((entry, index) => {
                if (entry.isIntersecting) {
                    setTimeout(() => {
                        entry.target.style.opacity = '1';
                        entry.target.style.transform = 'translateY(0)';
                    }, index * 100);
                }
            });
        }, observerOptions);

        // Initialize card observations
        [...happeningCards, ...inspirationCards].forEach(card => {
            card.style.opacity = '0';
            card.style.transform = 'translateY(20px)';
            card.style.transition = 'all 0.6s ease-out';
            cardObserver.observe(card);
        });

        // Enhanced search input interactions
        searchInput.addEventListener('focus', () => {
            searchInput.parentElement.style.transform = 'translateY(-2px)';
        });

        searchInput.addEventListener('blur', () => {
            searchInput.parentElement.style.transform = 'translateY(0)';
        });

        // Add keyboard navigation
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Tab') {
                document.body.classList.add('keyboard-navigation');
            }
        });

        document.addEventListener('mousedown', () => {
            document.body.classList.remove('keyboard-navigation');
        });

        // Add ripple animation styles
        const rippleStyle = document.createElement('style');
        rippleStyle.textContent = `
            @keyframes ripple {
                to {
                    transform: translate(-50%, -50%) scale(4);
                    opacity: 0;
                }
            }
            
            .prompt-chip.selected {
                background: rgba(30, 58, 138, 0.3);
                border-color: #1E3A8A;
                color: #1E3A8A;
                font-weight: 600;
            }
            
            .keyboard-navigation *:focus {
                outline: 2px solid #1E3A8A !important;
                outline-offset: 2px !important;
            }
        `;
        document.head.appendChild(rippleStyle);

        // Enhanced tooltip positioning
        const tooltip = document.querySelector('.tooltip-content');
        const tooltipTrigger = document.querySelector('.tooltip-trigger');

        tooltipTrigger.addEventListener('mouseenter', () => {
            const rect = tooltipTrigger.getBoundingClientRect();
            const tooltipRect = tooltip.getBoundingClientRect();
            
            // Adjust position if tooltip would go off screen
            if (rect.left + tooltipRect.width > window.innerWidth) {
                tooltip.style.left = 'auto';
                tooltip.style.right = '0';
                tooltip.style.transform = 'translateX(0)';
            }
        });

        // Add performance optimization for animations
        let ticking = false;

        function updateParallax() {
            const scrollTop = window.pageYOffset;
            
            inspirationCards.forEach((card, index) => {
                const speed = 0.3 + (index * 0.05);
                const yPos = -(scrollTop * speed);
                card.style.transform = `translateY(${yPos}px)`;
            });
            
            ticking = false;
        }

        window.addEventListener('scroll', () => {
            if (!ticking) {
                requestAnimationFrame(updateParallax);
                ticking = true;
            }
        });

        // Enhanced loading states and error handling
        window.addEventListener('load', () => {
            document.body.classList.add('loaded');
            
            // Simulate real data loading
            setTimeout(() => {
                happeningCards.forEach((card, index) => {
                    setTimeout(() => {
                        card.classList.add('loaded');
                    }, index * 200);
                });
            }, 500);
        });

        // Add network status handling
        window.addEventListener('online', () => {
            document.body.classList.add('online');
            document.body.classList.remove('offline');
        });

        window.addEventListener('offline', () => {
            document.body.classList.add('offline');
            document.body.classList.remove('online');
        });
    </script>
</body>
</html>
